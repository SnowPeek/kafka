<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kafka2.kafka2.dao.GetInfoDao">
    <select id="userInfo" resultType="com.kafka2.kafka2.entity.UserInfo" >
      select * from test.userInfo
    </select>

    <insert id="insertUser">
        insert into userInfo( name , password , phone ,address)
            VALUES(#{name},#{password},#{phone},#{address})
    </insert>
</mapper>
